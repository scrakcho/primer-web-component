    "@storybook/html": "^5.3.10",
    "@types/jest": "24.0.25",
    "@types/puppeteer": "1.19.0",
    "babel-loader": "^8.0.6",
    "jest": "24.9.0",
    "jest-cli": "24.9.0",
    "puppeteer": "1.19.0"
  },
  "license": "MIT",
